class Parent {
    void display() {
        System.out.println("Parent's display method");
    }
}

class Child extends Parent {
    void display() {
        super.display();
        System.out.println("Child's display method");
    }
}

public class MethodOverriding {
    public static void main(String[] args) {
        Child obj = new Child();
        obj.display();
    }
}
